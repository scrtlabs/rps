{
  "name": "rps-vue",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "dev18": "SET NODE_OPTIONS=--openssl-legacy-provider && nuxt",
    "dev18-linux": "export NODE_OPTIONS=--openssl-legacy-provider && nuxt",
    "build": "nuxt build",
    "build18": "SET NODE_OPTIONS=--openssl-legacy-provider && nuxt build",
    "build18-linux": "export NODE_OPTIONS=--openssl-legacy-provider && nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "generate18": "SET NODE_OPTIONS=--openssl-legacy-provider && nuxt generate",
    "generate18-linux": "export NODE_OPTIONS=--openssl-legacy-provider && nuxt generate",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:prettier",
    "lintfix": "prettier --write --list-different ."
  },
  "dependencies": {
    "@nuxtjs/dotenv": "^1.4.1",
    "core-js": "^3.19.3",
    "nuxt": "^2.15.8",
    "secretjs": "^1.5.1",
    "vue": "^2.6.14",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "vuetify": "^2.6.1",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@nuxtjs/vuetify": "^1.12.3",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1"
  }
}
